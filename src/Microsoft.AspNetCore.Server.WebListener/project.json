{
  "version": "0.1.0-rc2-final",
  "description": "ASP.NET 5 self-host web server.",
  "dependencies": {
    "Microsoft.AspNetCore.Hosting": "1.0.0-rc2-final",
    "Microsoft.Net.Http.Headers": "1.0.0-rc2-final",
    "Microsoft.Net.Http.Server": "0.1.0-rc2-final"
  },
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
      "dependencies": {
        "System.Security.Claims": "4.0.1-rc2-24027"
      },
      "imports": [
        "portable-net451+win8"
      ]
    }
  }
}